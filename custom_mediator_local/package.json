{
  "name": "custom_mediator",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "docker:start:dev": "docker compose -f 'docker-compose.development.yml' up -d --build --force-recreate",
    "docker:stop:dev": "docker compose -f 'docker-compose.development.yml' down",
    "docker:start:staging": "docker compose -f 'docker-compose.staging.yml' up -d --build --force-recreate",
    "docker:stop:staging": "docker compose -f 'docker-compose.staging.yml' down",
    "dev": "nodemon --config nodemon.json",
    "start": "ts-node -r tsconfig-paths/register -r dotenv/config src/index"
  },
  "dependencies": {
    "@tsconfig/recommended": "^1.0.2",
    "@types/fhir": "^0.0.37",
    "@types/node": "^20.3.1",
    "clickhouse": "^2.6.0",
    "dotenv": "^16.1.4",
    "express": "^4.18.2",
    "http-status": "^1.6.2",
    "module-alias": "^2.2.3",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.22",
    "openhim-mediator-utils": "^0.2.4",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.5.3",
    "typescript": "^5.1.3",
    "winston": "^3.9.0"
  },
  "devDependencies": {
    "ts-node": "^10.9.1"
  }
}
